pub type StatusCode = u16;
<% rows.each do |row| %>
pub const <%= row.desc %>: StatusCode = <%= row.value %>;	// <%= row.ref %>
<% end %>

pub fn to_str(a: StatusCode) -> Result<&'static str, ()> {
  match a {
    <% rows.each do |row| %>
    <%= row.value %> => Ok("<%= row.desc %>"),
    <% end %>
    _ => Err(()),
  }
}
